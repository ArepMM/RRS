<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <Order>2</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <V0>0.0005</V0>
        <V1>0.0005</V1>

        <!-- Скорость, ниже которой отключается реостатное торможение, км/ч (измеряется на первой оси) -->
        <U1>40.0</U1>
        <!-- Скорость, выше которой повышается давление в ТЦ, км/ч (измеряется на шестой оси) -->
        <U6>80.0</U6>

        <!-- Ток реостатного торможения, выше которого отпускает пневматический тормоз -->
        <I>100.0</I>

        <!-- Коэффицент к потоку на наполнение ТЦ -->
        <K1>1.0e-2</K1>
        <!-- Коэффицент к потоку на опорожнение ТЦ -->
        <K2>1.5e-2</K2>

        <!-- Коэффицент к потоку на наполнение нижней камеры -->
        <K3>3e-3</K3>
        <!-- Коэффицент к потоку на опорожнение нижней камеры -->
        <K4>4e-3</K4>

        <!-- Коэффицент к потоку на наполнение средней камеры -->
        <K5>3e-3</K5>
        <!-- Коэффицент к потоку на опорожнение средней камеры -->
        <K6>4e-3</K6>

        <A1>0.71</A1>
        <A2>1.0</A2>

        <k1>2</k1>
        <k2>2</k2>
        <k3>10</k3>
        <k4>10</k4>
    </Device>
</Config>
