<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>

        <!-- ТМ - тормозная магистраль -->
        <!-- ЗР - запасный резервуар -->
        <!-- ТЦ - тормозной цилиндр -->
        <!-- МК - магистральная камера, сообщена с ТМ -->
        <!-- РК - рабочая камера -->
        <!-- ЗК - золотниковая камера -->
        <!-- КДР - каналы дополнительной разрядки -->

        <!-- Моделируем давление в РК, ЗК, КДР -->
        <Order>3</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <!-- Состояние переключателя профиля пути -->
        <!-- 0 - горный; 1 - равнинный -->
        <switchProfile>1</switchProfile>
        <!-- Состояние переключателя загруженности вагона -->
        <!-- 0 - П (порожний); 1 - С (средний); 2 - Г (гружёный) -->
        <switchPayload>1</switchPayload>

        <!-- Объём РК, м^3 -->
        <v0>0.006</v0>
        <!-- Объём ЗК, м^3 -->
        <v1>0.0045</v1>
        <!-- Объём КДР (условно), м^3 -->
        <v2>0.0005</v2>

        <!-- Коэффициенты к перетокам через калиброванные отверстия -->
        <!-- В канале к обратному клапану зарядки ЗР из ТМ; d = 1.3 мм -->
        <k0>0.0013</k0>
        <!-- В каналах плунжера; d = 0.7 мм -->
        <k1>0.0007</k1>
        <!-- В канале между МК и ЗК через клапан мягкости; d = 0.9 мм -->
        <k2>0.0009</k2>
        <!-- В корпусе главного поршня между ЗК и РК; d = 0.55 мм -->
        <k3>0.00055</k3>
        <!-- В канале между РК и диафрагмой переключателя профиля; d = 0.6 мм -->
        <k4>0.0006</k4>
        <!-- Дополнительная разрядка МК в КДР; 6 x d = 2.0 мм -->
        <k5>0.012</k5>
        <!-- Дополнительная разрядка ЗК в КДР; 3 x d = 1.2 мм -->
        <k6>0.036</k6>
        <!-- В уравнительном поршне для предварительного наполнения ТЦ из КДР -->
        <k7>0.003</k7>
        <!-- В атмосферном осевом канале уравнительного поршня; d = 2.8 мм -->
        <k8>0.0028</k8>
        <!-- В атмосферном клапане; d = 0.9 мм -->
        <k9>0.0009</k9>
        <!-- В штоке главного поршня для медленного наполнения ТЦ; d = 1.7 мм -->
        <k10>0.0017</k10>
        <!-- В штоке главного поршня для быстрого наполнения ТЦ; 4 x d = 3.0 мм -->
        <k11>0.006</k11>
        <!-- В штоке главного поршня; d = 1.7 мм -->
        <k12>0.0017</k12>
        <!-- В дополнительном канале МК - плунжер (в воздухораспределителе 483М); d = 0.3 мм -->
        <!-- В КОДЕ НЕ РЕАЛИЗОВАНО -->
        <!-- <k13>0.0003</k13> -->

        <!-- Давление, эквивалентное усилиям пружин, для открытия клапанов, перемещения поршней и т.д., МПа -->

        <!-- Разность давлений между ЗК и КДР для открытия клапана мягкости -->
        <p0>0.35</p0>
        <!-- Давление в РК для отжатия диафрагмы переключателя профиля (равнинный режим) -->
        <p1>0.25</p1>

        <!-- Разность давлений между МК и ЗК, определяющая условное положение диафрагмы: -->
        <!-- начало связи МК и ЗК через плунжер -->
        <p2>0.015</p2>
        <!-- начало связи МК и РК через плунжер (равнинный режим) -->
        <p3>0.01</p3>
        <!-- начало разрядки ЗК в КДР через плунжер -->
        <p4>-0.01</p4>
        <!-- начало дополнительной разрядки МК в КДР -->
        <p5>-0.011</p5>
        <!-- начало дополнительной разрядки КДР через атмосферный клапан -->
        <p6>-0.015</p6>
        <!-- начало дополнительной разрядки ЗК в КДР -->
        <p7>-0.016</p7>

        <!-- Разность давлений между МК и КДР для отжатия манжеты дополнительной разрядки -->
        <p8>0.2</p8>

        <!-- Разность давлений между РК и ЗК, определяющая условное положение главного поршня: -->
        <!-- главный поршень разобщает РК и ЗК -->
        <p9>0.015</p9>
        <!-- главный поршень перекрывает КДР -->
        <p10>0.029</p10>
        <!-- через главный поршень начинается быстрое наполнение ТЦ -->
        <p11>0.03</p11>
        <!-- через главный поршень начинается медленное наполнение ТЦ -->
        <p12>0.04</p12>
        <!-- главный поршень смещается в крайнее положение -->
        <p13>0.11</p13>

        <!-- Эквивалентное давление на уравнительный поршень: -->
        <!-- от основной пружины при положении главного поршня, соответсвующем началу наполнения ТЦ -->
        <p14>0.08</p14>
        <!-- от основной пружины в крайнем положении главного поршня -->
        <p15>0.18</p15>
        <!-- от пружины гружёного режима при положении главного поршня, соответсвующем началу наполнения ТЦ -->
        <p16>0.03</p16>
        <!-- от пружины гружёного режима в крайнем положении главного поршня -->
        <p17>0.23</p17>

        <!-- Коэффициент чувствительности к разности сил на уравнительном поршне -->
        <A>1.0</A>

    </Device>
</Config>
