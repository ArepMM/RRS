<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <!-- Моделируем давление в камере дополнительной разрядки ТМ -->
        <Order>1</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <!-- Режим: 0 - короткосоставный (К), 1 - длинносоставный (Д), 2 - ускоритель экстренного выключен (УВ) -->
        <LongTrainMode>0</LongTrainMode>

        <!-- Объем камеры дополнительной разрядки ТМ, м^3 -->
        <Vkd>1e-3</Vkd>

        <!-- Условная задержка магистрального поршня и отсекательного золотника из-за трения (относительно разницы давлений ЗР и ТМ, МПа) -->
        <disjunction_z_eps>0.03</disjunction_z_eps>

        <!-- Условный зазор главного золотника относительно магистрального поршня (относительно разницы давлений ЗР и ТМ, МПа) -->
        <main_z_eps>0.02</main_z_eps>

        <!-- Разница давлений ЗР и ТМ, при которой поршень разобщает связь ЗР и ТМ, и начинается торможение, МПа -->
        <p1>0.005</p1>
        <!-- Разница давлений ЗР и ТМ, при которой поясок поршня отходит от втулки, начиная быструю зарядку ЗР, МПа -->
        <p2>-0.02</p2>
        <!-- Конец отпускного положения главного золотника (разницы давлений ЗР и ТМ), при котором разобщается связь ТЦ и КДР с атмосферой, МПа -->
        <p3>0.01</p3>
        <!-- Начало экстренного положения главного золотника (разница давлений ЗР и ТМ), при котором прекращается управление КДР и ТЦ, МПа -->
        <p4>0.03</p4>
        <!-- Разница положений золотников, при которой отсекательный золотник подключает КДР к ТМ и ЗР к ТЦ, МПа -->
        <p5>0.0</p5>

        <!-- Коэффициенты перетока между тормозной магистралью и запасным резервуаром: -->
        <!-- через два отверстия диаметром 2.0 мм в пояске поршня; -->
        <K1>1.0e-3</K1>
        <!-- дополнительно через зазор между пояском поршня и втулкой для быстрой зарядки ЗР, -->
        <!-- с учётом общего ограничения потока тремя отверстиями диаметром 1.25 мм -->
        <K2>0.8e-3</K2>
        <!-- Коэффициент перетока между тормозной магистралью и камерой дополнительной разрядки ТМ -->
        <K3>2.0e-3</K3>
        <!-- Коэффициент потока из камеры дополнительной разрядки ТМ в атмосферу -->
        <K4>2.0e-3</K4>
        <!-- Коэффициент наполнения ТЦ в режиме К -->
        <K5>5.0e-3</K5>
        <!-- Коэффициент наполнения ТЦ в режимах Д и УВ -->
        <K6>2.0e-3</K6>
        <!-- Коэффициент отпуска ТЦ в режиме К -->
        <K7>5.0e-3</K7>
        <!-- Коэффициент отпуска ТЦ в режимах Д и УВ -->
        <K8>2.0e-3</K8>

        <!-- Коэффициент чувствительности к зазору между пояском поршня и втулкой для быстрой зарядки ЗР -->
        <A1>0.3</A1>
        <!-- Коэффициент чувствительности к разнице положений золотников при управлении камерой дополнительной разрядки и перекрышей потока в ТЦ -->
        <A2>0.3</A2>





        <!-- Коэффициент перетока между тормозной магистралью и ускорительной камерой -->
        <K2>3.2e-3</K2>

        <!-- Коэффициент перетока между тормозной магистралью и ускорительной камерой-->
        <!-- по широкому каналу при сверхзарядном давлении -->
        <K3>4.0e-2</K3>

        <!-- Коэффициент наполнения ТЦ в режиме Д -->
        <K4>2.0e-3</K4>

        <!-- Коэффициент наполнения через тормозной клапан (режим К и начало режима Д) -->
        <K5>5.0e-3</K5>

        <!-- Коэффициент отпуска в режиме Д -->
        <K6>2.0e-3</K6>

        <!-- Коэффициент отпуска через тормозной клапан (режим К и конец режима Д) -->
        <K7>5.0e-3</K7>

        <!-- Коэффициент дополнительной разрядки ТМ -->
        <K8>2.0e-3</K8>

        <!-- Коэффициент экстренной разрядки ТМ -->
        <K9>1.0e-2</K9>

        <!-- Коэффициент к дополнительной разрядке ТМ от главного поршня -->
        <k2>1000.0</k2>




    </Device>
</Config>