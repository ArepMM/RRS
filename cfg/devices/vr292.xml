<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <Order>1</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <!-- Режим: 0 - короткосоставный (К), 1 - длинносоставный (Д), 2 - ускоритель экстренного выключен (УВ) -->
        <LongTrainMode>0</LongTrainMode>

        <!-- Объем камеры дополнительной разрядки ТМ -->
        <Vkd>1e-3</Vkd>








        <!-- Давление срабатывания клапана в камере У2 (разобщение ТМ и ЗР) -->
        <pu2>0.037</pu2>

        <!-- Давление запирания тормозного клапана в длинносоставном режиме -->
        <pbv>0.15</pbv>

        <!-- Давление открытия срывного клапана ускорения экстренного торможения -->
        <psv>0.025</psv>

        <!-- Давление открытия широкого канала в ускорительную камеру (сверхзарядное) -->
        <pwv>0.48</pwv>

        <!-- Зона нечувствительности для главного поршня -->
        <s1_min>-0.015</s1_min>
        <s1_max>0.015</s1_max>

        <!-- Коэффициент перетока между тормозной магистралью и запасным резервуаром -->
        <K1>1.8e-3</K1>

        <!-- Коэффициент перетока между тормозной магистралью и ускорительной камерой -->
        <K2>3.2e-3</K2>

        <!-- Коэффициент перетока между тормозной магистралью и ускорительной камерой-->
        <!-- по широкому каналу при сверхзарядном давлении -->
        <K3>4.0e-2</K3>

        <!-- Коэффициент наполнения ТЦ в режиме Д -->
        <K4>2.0e-3</K4>

        <!-- Коэффициент наполнения через тормозной клапан (режим К и начало режима Д) -->
        <K5>5.0e-3</K5>

        <!-- Коэффициент отпуска в режиме Д -->
        <K6>2.0e-3</K6>

        <!-- Коэффициент отпуска через тормозной клапан (режим К и конец режима Д) -->
        <K7>5.0e-3</K7>

        <!-- Коэффициент дополнительной разрядки ТМ -->
        <K8>2.0e-3</K8>

        <!-- Коэффициент экстренной разрядки ТМ -->
        <K9>1.0e-2</K9>

        <!-- Коэффициент к наполнению ТЦ от главного поршня -->
        <k1>200.0</k1>

        <!-- Коэффициент к дополнительной разрядке ТМ от главного поршня -->
        <k2>1000.0</k2>




    </Device>
</Config>