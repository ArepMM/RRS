<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <Order>1</Order>
        <SubStepNum>1</SubStepNum>
        <Solver>euler</Solver>

        <!-- Темп наполнения рабочей камеры из ЗР -->
        <K1>5.0e-4</K1>

        <!-- Темп опорожнения рабочей камеры  -->
        <K2>6.0e-4</K2>

        <!-- Темп наполнения ТЦ через реле давления-->
        <K3>5.0e-2</K3>

        <!-- Темп опорожнения ТЦ через реле давления -->
        <K4>5.0e-2</K4>

        <!-- Коэффициент к клапану наполнения ТЦ -->
        <k1>20.0</k1>
        <!-- Коэффициент к клапану опорожнения ТЦ -->
        <k2>20.0</k2>

        <!-- Объем рабочей камеры -->
        <V0>1.5e-3</V0>

        <!-- Сопротивление катушек вентилей, Ом -->
        <R>400.0</R>
        <!-- Индуктивность катушек вентилей, Гн -->
        <L>0.5</L>
        <!-- Ток включения вентилей, А -->
        <I_on>0.1</I_on>

    </Device>
</Config>