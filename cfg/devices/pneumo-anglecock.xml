<?xml version="1.0" encoding="UTF-8"?>
<Config>
    <Device>
        <Order>0</Order>

        <!-- Начальное состояние концевого крана: 0 - закрыт, 1 - открыт -->
        <IsOpened>1</IsOpened>

        <!-- Время переключения концевого крана, с -->
        <SwitchTime>0.5</SwitchTime>

        <!-- Коэффициент перетока из рукава в магистраль через открытый концевой кран -->
        <k>0.05</k>

        <!-- Коэффициент разрядки из рукава в атмосферу при закрытом концевом кране -->
        <kAtm>0.01</kAtm>
    </Device>
</Config>